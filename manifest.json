{
  "name": "CB Priority Pages",
  "description": "Test a CL Site's Priority Pages",
  "version": "1.1.2",
  "manifest_version": 3,
  "options_page": "/html/options.html",
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "action": {
    "default_popup": "/html/popup.html",
    "default_icon": {
      "16": "/images/128.png",
      "32": "/images/128.png",
      "48": "/images/128.png",
      "128": "/images/128.png"
    }
  },
  "icons": {
    "16": "/images/128.png",
    "32": "/images/128.png",
    "48": "/images/128.png",
    "128": "/images/128.png"
  }
}